{
  "name": "hurado",
  "version": "2.0.0",
  "description": "NOI.PH's online judge",
  "scripts": {
    "build": "next build",
    "start": "next start -- -p 10001",
    "worker": "node --conditions=react-server -r ts-node/register ./src/worker/worker.ts",
    "dev": "next dev -p 10000",
    "dev:worker": "nodemon --watch 'src/**' --ext '*' --exec 'node --conditions=react-server -r ts-node/register ./src/worker/worker.ts'",
    "admin:create": "node --conditions=react-server -r ts-node/register src/db/scripts/devtools admin:create",
    "admin:update": "node --conditions=react-server -r ts-node/register src/db/scripts/devtools admin:update",
    "contest:create": "node --conditions=react-server -r ts-node/register src/db/scripts/devtools contest:create",
    "problem-set:create": "node --conditions=react-server -r ts-node/register src/db/scripts/devtools problem-set:create",
    "task:create": "node --conditions=react-server -r ts-node/register src/db/scripts/devtools task:create",
    "storage:init": "node --conditions=react-server -r ts-node/register src/db/scripts/devtools storage:init",
    "db:reset": "node --conditions=react-server -r ts-node/register src/db/scripts/devtools db:reset",
    "db:recreate": "node --conditions=react-server -r ts-node/register src/db/scripts/devtools db:recreate",
    "db:migrate": "node --conditions=react-server -r ts-node/register src/db/scripts/devtools db:migrate",
    "db:seed": "node --conditions=react-server -r ts-node/register src/db/scripts/devtools db:seed",
    "lint": "next lint",
    "lint:fix": "next lint --fix && prettier src --write",
    "format": "prettier src --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noiph/hurado.git"
  },
  "keywords": [
    "online-judge"
  ],
  "author": "NOI.PH",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/noiph/hurado/issues"
  },
  "homepage": "https://github.com/noiph/hurado#readme",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.723.0",
    "@aws-sdk/lib-storage": "^3.723.0",
    "@azure/storage-blob": "^12.24.0",
    "@hookform/resolvers": "^3.10.0",
    "@inquirer/prompts": "^7.2.1",
    "@monaco-editor/react": "^4.6.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/react-dom": "^18.3.5",
    "@unified-latex/unified-latex": "^1.7.1",
    "axios": "^1.6.8",
    "bcryptjs": "^2.4.3",
    "boxicons": "^2.1.4",
    "bullmq": "^5.12.9",
    "classnames": "^2.5.1",
    "cookies-next": "^4.1.1",
    "dotenv": "^16.4.5",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "katex": "^0.16.11",
    "kysely": "^0.27.3",
    "next": "^14.1.3",
    "overlayscrollbars-react": "^0.5.6",
    "pg": "^8.11.5",
    "pg-query-stream": "^4.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.2",
    "react-toastify": "^10.0.5",
    "sharp": "^0.33.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@svgr/webpack": "^8.1.0",
    "@types/eslint__js": "^8.42.3",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/katex": "^0.16.7",
    "@types/node": "20.11.24",
    "@types/pg": "^8.11.6",
    "@types/react": "^18.2.61",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "14.1.4",
    "eslint-config-prettier": "^9.1.0",
    "nodemon": "^3.1.4",
    "postcss": "^8.4.41",
    "prettier": "^3.2.5",
    "server-only": "^0.0.1",
    "tailwindcss": "^3.4.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.0.1"
  }
}
